{
  "name": "ngx-image-gallery",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng build ngx-image-gallery --watch",
    "build": "ng build ngx-image-gallery",
    "start:dev": "ng build ngx-image-gallery --watch --configuration development",
