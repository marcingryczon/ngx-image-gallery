    "start-demo-app": "cd ./projects/demo-app && ng serve",
    "lint": "ng lint",
    "format": "npx prettier ./projects/ngx-image-gallery/src/lib/**/*.{js,jsx,ts,tsx,html,css,scss} --write"
  },
  "private": true,
